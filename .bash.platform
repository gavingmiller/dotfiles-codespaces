# OS specific configurations
# Hat tip to https://github.com/mokhan
OS=$(uname)
if [ "$OS" = "Linux" ]
then
  source ~/.bash/linux/aliases
  PS1="\t ${RED}\h${NORMAL} \W\$(grb_git_prompt)\$(job_count)\$ "
elif [ "$OS" = "Darwin" ]
then
  source ~/.bash/osx/aliases
  PS1="\t \W\$(grb_git_prompt)\$(job_count)\$ "
fi

